<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="roomie.model.evaluation.LandlordEvaluation" table="LandlordEvaluation" lazy="false">
        <composite-id>
            <key-many-to-one name="landlord" column="LandlordID" class="roomie.model.landlord.Landlord">
            </key-many-to-one>
            <key-many-to-one name="tenant" column="TenantID" class="roomie.model.tenant.Tenant">
            </key-many-to-one>
        </composite-id>
        <property name="cleanliness" column="Cleanliness" type="double" not-null="true" lazy="false"/>
        <property name="payment" column="Payment" type="double" not-null="true" lazy="false"/>
        <property name="care" column="Care" type="double" not-null="true" lazy="false"/>
    </class>
</hibernate-mapping>
